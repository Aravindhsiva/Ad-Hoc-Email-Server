{
  "name": "ahem-server",
  "version": "1.0.0",
  "license": "LGPL-3",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"node ahem.js\" \"ng serve --proxy-config proxy.conf.json\"",
    "test": "ng test",
    "testServer": "node_modules/jest/bin/jest.js **/*/serverapp.spec.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "npm install && npm rebuild node-sass && ng build",
    "buildDev": "npm install && npm rebuild node-sass && ng build --dev",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/universalServer --proxy-config proxy.conf.json",
    "build:client-and-server-bundles": "ng build --prod && ng run ahem-universal:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors --display-error-details"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.8",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.8",
    "@angular/compiler": "^6.1.8",
    "@angular/core": "^6.1.8",
    "@angular/forms": "^6.1.8",
    "@angular/http": "^6.1.8",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.8",
    "@angular/platform-browser-dynamic": "^6.1.8",
    "@angular/platform-server": "^6.1.9",
    "@angular/router": "^6.1.8",
    "@fortawesome/angular-fontawesome": "^0.2.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@nguniversal/express-engine": "^6.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.1.0",
    "angular-swagger-ui": "^0.5.5",
    "angulartics2": "^6.3.0",
    "core-js": "^2.4.1",
    "jsonwebtoken": "^8.3.0",
    "mailparser": "^2.3.4",
    "moment": "^2.22.2",
    "mongodb": "^3.1.6",
    "morgan": "^1.9.1",
    "ngx-moment": "^3.1.0",
    "nodemon": "^1.18.4",
    "rxjs": "^6.3.2",
    "smtp-server": "^3.4.7",
    "swagger-ui-express": "^4.0.1",
    "ts-loader": "^5.2.1",
    "winston": "^3.1.0",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.5",
    "@angular/cli": "^6.2.3",
    "@angular/compiler-cli": "^6.1.8",
    "@angular/language-service": "^6.1.8",
    "@types/jasmine": "^2.8.9",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~10.11.0",
    "codelyzer": "~4.4.4",
    "jasmine": "^3.2.0",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jasmine-ts": "^0.3.0",
    "jest": "^23.6.0",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.3.1",
    "nodemailer": "^4.6.8",
    "nyc": "^13.1.0",
